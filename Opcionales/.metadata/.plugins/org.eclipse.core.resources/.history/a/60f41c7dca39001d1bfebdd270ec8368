package ejercicio0102;

import java.io.IOException;
import java.io.PrintWriter;

public class Ejercicio0102 {

	public static void main(String[] args) {
		try (PrintWriter pw = new PrintWriter("datos.txt")) {
			for (int i=1; i<=10; i++)
			{
				pw.println(i);
			}			
		}catch (IOException ioe) {
			ioe.printStackTrace();
		}catch (Exception e) {
			e.printStackTrace();
		}
	}
}
